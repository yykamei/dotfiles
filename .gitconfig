[core]
	excludesfile = /home/kamei/.gitignore_global
[log]
	abbrevCommit = off
	decorate = true
[color "status"]
	untracked = "red bold "
[credential]
	helper = cache --timeout=3600
[rebase]
	autosquash = true
[merge]
	tool = vimdiff
[mergetool]
	prompt = false
[user]
	name = Yutaka Kamei
[tag]
	sort = version:refname
